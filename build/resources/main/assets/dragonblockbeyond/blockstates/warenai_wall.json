{
  "multipart": [
    {
      "when": {"up": "true"},
      "apply": {"model": "dragonblockbeyond:block/warenai_wall/posts/warenai_wall_post"}
    },
    {
      "when": {"north": "low"},
      "apply": {"model": "dragonblockbeyond:block/warenai_wall/low_sides/warenai_wall_side", "uvlock": true}
    },
    {
      "when": {"east": "low"},
      "apply": {"model": "dragonblockbeyond:block/warenai_wall/low_sides/warenai_wall_side", "y": 90, "uvlock": true}
    },
    {
      "when": {"south": "low"},
      "apply": {"model": "dragonblockbeyond:block/warenai_wall/low_sides/warenai_wall_side", "y": 180, "uvlock": true}
    },
    {
      "when": {"west": "low"},
      "apply": {"model": "dragonblockbeyond:block/warenai_wall/low_sides/warenai_wall_side", "y": 270, "uvlock": true}
    },
    {
      "when": {"north": "tall"},
      "apply": {"model": "dragonblockbeyond:block/warenai_wall/tall_sides/warenai_wall_side_tall", "uvlock": true}
    },
    {
      "when": {"east": "tall"},
      "apply": {"model": "dragonblockbeyond:block/warenai_wall/tall_sides/warenai_wall_side_tall", "y": 90, "uvlock": true}
    },
    {
      "when": {"south": "tall"},
      "apply": {"model": "dragonblockbeyond:block/warenai_wall/tall_sides/warenai_wall_side_tall", "y": 180, "uvlock": true}
    },
    {
      "when": {"west": "tall"},
      "apply": {"model": "dragonblockbeyond:block/warenai_wall/tall_sides/warenai_wall_side_tall", "y": 270, "uvlock": true}
    }
  ]
}